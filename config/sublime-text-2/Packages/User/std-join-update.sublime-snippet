UPDATE t00
  SET t00.veld1 = t1.veld2
  FROM dbo.Table1 AS t00
  INNER JOIN dbo.Table2 AS t10
  ON t00.veld3 = t10.veld4
  WHERE t00.veld5 = '110'
]]></content>
